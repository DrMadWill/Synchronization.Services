<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <RootNamespace>DrMadWill.Event.Synchronization.Service</RootNamespace>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>DrMadWill.Event.Synchronization.Services</Title>
        <Authors>DrMadWill</Authors>
        <PackageIcon>avatar.png</PackageIcon>
        <Version>8.2</Version>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="DrMW.EventBus.Core" Version="2.0.0" />
      <PackageReference Include="DrMW.Repositories" Version="1.7.0" />
      <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="6.0.1" />
    </ItemGroup>

    <ItemGroup>
      <None Update="Dr.png">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
      </None>
    </ItemGroup>

    <ItemGroup>
      <None Update="avatar.png">
        <Pack>True</Pack>
        <PackagePath></PackagePath>
      </None>
    </ItemGroup>

</Project>
